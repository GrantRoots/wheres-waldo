generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Records {
  id Int @id @default(autoincrement())
  name String
  time DateTime
}

//I WOULD NEED TO STORE THE COORDINATED EVERY NEW GAME
model Positions {
  id Int @id @default(autoincrement())
  waldo Json
  wizard Json
  wilma Json
  odlaw Json
  woof Json
}